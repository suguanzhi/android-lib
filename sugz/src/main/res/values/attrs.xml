<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="width" format="dimension" />
    <attr name="height" format="dimension" />
    <attr name="text_size" format="dimension" />
    <attr name="background" format="reference" />
    <attr name="background_color" format="color" />
    <attr name="padding" format="dimension" />
    <attr name="text_color" format="reference" />
    <attr name="radius" format="dimension" />
    <attr name="value" format="integer" />
    <attr name="max_value" format="integer" />
    <attr name="color" format="color" />
    <attr name="border_width" format="dimension" />
    <attr name="border_color" format="color" />
    <attr name="radius_style">
        <enum name="wrap" value="0" />
        <enum name="circle" value="1" />
    </attr>

    <declare-styleable name="Titlebar">
        <attr name="height" />
        <attr name="title" format="string" />
        <attr name="text_size" />
        <attr name="back" format="boolean" />
        <attr name="left" format="reference" />
        <attr name="left_text" format="string" />
        <attr name="right" format="reference" />
        <attr name="right_text" format="string" />
        <attr name="background" />
    </declare-styleable>

    <declare-styleable name="PagerNavigation">
        <attr name="items" format="reference|string" />
        <attr name="text_size" />
        <attr name="line" format="reference" />
        <attr name="line_height" format="dimension" />
        <attr name="line_margin" format="dimension" />
        <attr name="nav_width" format="dimension" />
        <attr name="padding" />
        <attr name="text_color" />
        <attr name="text_color_selected" format="reference" />
        <attr name="background" />
    </declare-styleable>

    <declare-styleable name="LoadingView">
        <attr name="color1" format="reference" />
        <attr name="color2" format="reference" />
    </declare-styleable>

    <declare-styleable name="LoadResultView">
        <attr name="icon" format="reference" />
        <attr name="background" />
        <attr name="text" format="string" />
        <attr name="text_size" />
        <attr name="text_color" />
        <attr name="btn_text" format="dimension" />
        <attr name="btn_text_size" format="dimension" />
        <attr name="btn_text_color" format="color" />
        <attr name="btn_background" format="reference" />
    </declare-styleable>

    <declare-styleable name="NumberEditText">
        <attr name="width" />
        <attr name="height" />
        <attr name="background" />
        <attr name="text_size" />
        <attr name="text_color" />
        <attr name="add" format="reference" />
        <attr name="sub" format="reference" />
        <attr name="value" />
        <attr name="min" format="integer" />
        <attr name="max" format="integer" />
        <attr name="input_background" format="reference" />
        <attr name="add_sub_padding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CornerImageView">
        <attr name="radius" />
    </declare-styleable>

    <declare-styleable name="SuButton">
        <attr name="radius" />
        <attr name="radius_style" />
        <attr name="style">
            <enum name="normal" value="1" />
            <enum name="warn" value="2" />
            <enum name="danger" value="3" />
            <enum name="confirm" value="4" />
            <enum name="disable" value="5" />
        </attr>
        <attr name="background_color" />
        <attr name="border_color" />
        <attr name="border_width" />
        <attr name="state_animator" format="boolean" />
        <attr name="stroke" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CircleProgressBar">
        <attr name="ring_width" format="dimension" />
        <attr name="ring_color" format="color" />
        <attr name="ring_background_color" format="color" />
        <attr name="circle_in_color" format="color" />
        <attr name="max_value" />
        <attr name="value" />
        <attr name="text_size" />
        <attr name="text_color" />

    </declare-styleable>

    <declare-styleable name="SuEditText">
        <attr name="android:layout_height" />
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="android:textStyle" />
        <attr name="android:src" />
        <attr name="background_color" />
        <attr name="border_width" />
        <attr name="border_color" />
        <attr name="radius_style" />
        <attr name="radius" />
        <attr name="clear_padding" format="dimension" />
    </declare-styleable>
</resources>