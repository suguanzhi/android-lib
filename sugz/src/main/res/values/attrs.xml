<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="width" format="dimension"></attr>
    <attr name="height" format="dimension"></attr>
    <attr name="text_size" format="dimension" />
    <attr name="background" format="reference"></attr>
    <attr name="padding" format="dimension"></attr>
    <attr name="text_color" format="reference" />
    <attr name="radius" format="dimension" />

    <declare-styleable name="titlebar">
        <attr name="height"></attr>
        <attr name="title" format="string"></attr>
        <attr name="text_size"></attr>
        <attr name="back" format="reference"></attr>
        <attr name="left" format="reference"></attr>
        <attr name="left_text" format="string"></attr>
        <attr name="right" format="reference"></attr>
        <attr name="right_text" format="string"></attr>
        <attr name="background"></attr>
    </declare-styleable>

    <declare-styleable name="pager_nav">
        <attr name="items" format="reference|string" />
        <attr name="text_size" />
        <attr name="line" format="reference"></attr>
        <attr name="line_height" format="dimension"></attr>
        <attr name="line_margin" format="dimension"></attr>
        <attr name="nav_width" format="dimension"></attr>
        <attr name="padding"></attr>
        <attr name="text_color" />
        <attr name="text_color_selected" format="reference" />
        <attr name="background"></attr>
    </declare-styleable>

    <declare-styleable name="loadingview">
        <attr name="color1" format="reference" />
        <attr name="color2" format="reference" />
    </declare-styleable>

    <declare-styleable name="LoadResultView">
        <attr name="icon" format="reference"></attr>
        <attr name="background"></attr>
        <attr name="text" format="string"></attr>
        <attr name="text_size"></attr>
        <attr name="text_color"></attr>
        <attr name="btn_text" format="dimension"></attr>
        <attr name="btn_text_size" format="dimension"></attr>
        <attr name="btn_text_color" format="color"></attr>
        <attr name="btn_background" format="reference"></attr>
    </declare-styleable>

    <declare-styleable name="InputEditView">
        <attr name="width" />
        <attr name="height" />
        <attr name="background" />
        <attr name="text_size" />
        <attr name="text_color" />
        <attr name="add" format="reference" />
        <attr name="sub" format="reference" />
        <attr name="value" format="integer" />
        <attr name="min" format="integer" />
        <attr name="max" format="integer" />
        <attr name="input_background" format="reference" />
        <attr name="add_sub_padding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CornerImageView">
        <attr name="radius" />
    </declare-styleable>

    <declare-styleable name="SuButton">
        <attr name="radius" />
        <attr name="style">
            <enum name="normal" value="1" />
            <enum name="warn" value="2" />
            <enum name="danger" value="3" />
            <enum name="confirm" value="4" />
            <enum name="disable" value="5" />
            <enum name="primary" value="6" />
            <enum name="warn_stroke" value="12" />
            <enum name="danger_stroke" value="13" />
            <enum name="confirm_stroke" value="14" />
            <enum name="primary_stroke" value="16" />
        </attr>
        <attr name="android:layout_height"/>
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="reference" />
        <attr name="state_animator" format="boolean" />
    </declare-styleable>
</resources>